<%= form_for [@company, @project] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :due_date %>
  <%= f.date_field :due_date %>

  <%= f.label :level %>
  <%= f.text_field :level %>

  <%= f.button %>
<% end %>