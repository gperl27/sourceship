<h2>Edit</h2>

<%= form_for @company do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :url %>
  <%= f.text_area :url %>

  <%= f.label :username %>
  <%= f.text_area :username %>

  <%= f.label :password %>
  <%= f.text_area :password %>

  <%= f.button %>
<% end %>